type User {
  id: ID!
  firebaseUID: String!
  firstName: String
  lastName: String
  email: String!
}

type Project {
  id: ID!
  name: String!
}

type Query {
  me: User
  myProjects: [Project!]!
}

input NewProject {
  name: String!
}

type Mutation {
  createProject(input: NewProject!): Project!
}
